{
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "server.py"
        },
        {
            "src": "/avatar",
            "dest": "/avatar.html"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}